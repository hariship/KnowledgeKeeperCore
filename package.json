    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:dev": "NODE_ENV=development ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:prod": "NODE_ENV=production node ./node_modules/typeorm/cli.js",
    "migration:run:dev": "npm run typeorm:dev -- migration:run -d src/db/data_source.ts",
    "migration:run:prod": "npm run typeorm:prod -- migration:run -d dist/db/data_source.ts",
    "migration:generate": "npm run typeorm migration:generate -d src/db/data_source.ts",
    "migration:create": "npm run typeorm migration:create -d src/db/data_source.ts"
